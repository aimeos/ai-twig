{
	"name": "aimeos/ai-twig",
	"description": "Twig template engine adapter for Aimeos e-commerce components",
	"keywords": ["twig", "templates", "aimeos", "extension"],
	"type": "aimeos-extension",
	"license": "LGPL-3.0-or-later",
	"support": {
		"source": "https://github.com/aimeos/ai-twig",
		"issues": "https://github.com/aimeos/ai-twig/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": "^8.0.11",
		"twig/twig": "~3.5",
		"aimeos/aimeos-core": "2024.10.*"
	},
	"require-dev": {
		"phpunit/phpunit": "~10.0||~11.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\Base\\": "src"
		},
		"classmap": [
			"src"
		]
	}
}